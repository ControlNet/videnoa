{
  "name": "Anime 4x Upscale",
  "description": "Upscale anime to 4x resolution using RealESRGAN x4plus anime model with tiling",
  "workflow": {
    "nodes": [
      {
        "id": "workflow_input",
        "node_type": "WorkflowInput",
        "params": {
          "ports": [
            {
              "name": "input",
              "port_type": "Path"
            },
            {
              "name": "output",
              "port_type": "Path"
            }
          ]
        }
      },
      {
        "id": "input",
        "node_type": "VideoInput",
        "params": {}
      },
      {
        "id": "sr",
        "node_type": "SuperResolution",
        "params": {
          "model_path": "models/RealESRGAN_x4plus_anime_6B.onnx",
          "scale": 4,
          "tile_size": 320,
          "backend": "tensorrt"
        }
      },
      {
        "id": "output",
        "node_type": "VideoOutput",
        "params": {
          "codec": "libx265",
          "crf": 16,
          "pixel_format": "yuv420p10le"
        }
      }
    ],
    "connections": [
      {
        "from_node": "workflow_input",
        "from_port": "input",
        "to_node": "input",
        "to_port": "path",
        "port_type": "Path"
      },
      {
        "from_node": "workflow_input",
        "from_port": "output",
        "to_node": "output",
        "to_port": "output_path",
        "port_type": "Path"
      },
      {
        "from_node": "input",
        "from_port": "frames",
        "to_node": "sr",
        "to_port": "frames",
        "port_type": "VideoFrames"
      },
      {
        "from_node": "sr",
        "from_port": "frames",
        "to_node": "output",
        "to_port": "frames",
        "port_type": "VideoFrames"
      },
      {
        "from_node": "input",
        "from_port": "source_path",
        "to_node": "output",
        "to_port": "source_path",
        "port_type": "Path"
      }
    ],
    "interface": {
      "inputs": [
        {
          "name": "input",
          "port_type": "Path"
        },
        {
          "name": "output",
          "port_type": "Path"
        }
      ],
      "outputs": []
    }
  }
}
